        android:textColor="@color/colorPrimary"
        android:textSize="@dimen/sp_16" />

    <WebView
        android:id="@+id/wv_mihoyo_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>



</LinearLayout>